    "translation:extract": "node node_modules/vue-webpack-gettext/extract --output ./languages/template.pot --src ./resources/js/",
    "translation:compile": "cross-env node_modules/.bin/gettext-compile --output ./languages/translations.json ./languages/locale/en_GB.po ./languages/locale/hu_HU.po ./languages/locale/id_ID.po ./languages/locale/pt_BR.po",
